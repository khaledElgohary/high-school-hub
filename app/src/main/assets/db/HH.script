CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE PUBLIC.HIGHSCHOOLS(NAME VARCHAR(MAX) NOT NULL PRIMARY KEY)
CREATE MEMORY TABLE PUBLIC.USERS(USERID INTEGER NOT NULL PRIMARY KEY,FIRSTNAME VARCHAR(50), LASTNAME VARCHAR(50), BIO VARCHAR(MAX), MARITALSTATUS VARCHAR(30), HIGHSCHOOLNAME VARCHAR(MAX), CONSTRAINT CONSTRAINT1 FOREIGN KEY(HIGHSCHOOLNAME) REFERENCES HIGHSCHOOLS(NAME))
CREATE MEMORY TABLE PUBLIC.REQUESTS(SENDER INTEGER NOT NULL,RECIPIENT INTEGER NOT NULL, ACCEPTED BIT,CONSTRAINT CONSTRAINT1 PRIMARY KEY(SENDER,RECIPIENT),CONSTRAINT CONSTRAINT2 FOREIGN KEY(SENDER) REFERENCES USERS(USERID),CONSTRAINT CONSTRAINT3 FOREIGN KEY(RECIPIENT) REFERENCES USERS(USERID))
CREATE MEMORY TABLE PUBLIC.CONNECTIONS(USER INTEGER NOT NULL, CONNECTEDUSER INTEGER NOT NULL, CONSTRAINT CONSTRAINT1 PRIMARY KEY(USER, CONNECTEDUSER), CONSTRAINT CONSTRAINT2 FOREIGN KEY(USER) REFERENCES USERS(USERID), CONSTRAINT CONSTRAINT3 FOREIGN KEY(CONNECTEDUSER) REFERENCES USERS(USERID))
CREATE MEMORY TABLE PUBLIC.SOCIALS(USER INTEGER NOT NULL, TYPE VARCHAR(50) NOT NULL, LINK VARCHAR(MAX) NOT NULL, CONSTRAINT CONSTRAINT1 PRIMARY KEY(USER, TYPE, LINK), CONSTRAINT CONSTRAINT2 FOREIGN KEY(USER) REFERENCES USERS(USERID))
